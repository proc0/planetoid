Play(duration=0.05s) {
    Oscillator(wave=$triangle, freq=[825, 55])
    Envelope(attack=0.01s, sustain=0.02s, release=0.05s)
    Delay(duration=0.04s)
    Vibrato(freqModulation=40) {
        Oscillator(wave=$sawtooth, freq=3)
    }
    Oscillator(wave=$sine, freq=40)
    HighPass(freq=100)
    LowPass(freq=900)
    Gain(volume=0.3)
}